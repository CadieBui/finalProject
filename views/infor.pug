body
  include includes/navigation.pug

  .page
    .form( method='post',action='/inforRoute',name='inforForm')
     .container
        button#infor_headshot()
        .col
        .infor_txt
            p Email
        .infor_data
            input(type='email' )
        .col
        .infor_txt
            p Name
        .infor_data
            input(type='text' )
        .col
        .infor_txt
            p Password
        .infor_data
            input(type='text')
        .col
        .infor_txt
            p Birthday
        .infor_data
            input(type='date' )
        .col
        .infor_txt
            p Sex
        .infor_data.infor_sex
            input(type='button' value='Female' )
            input(type='button' value='Male' )
        .col
        .infor_txt
            p Height
        .infor_data
            input(type='text')
        .col
        .infor_txt
            p Weight
        .infor_data
            input(type='text' readonly='true')
        .col
        .infor_txt
            p Age
        .infor_data
            input(type='text' readonly='true')
        .col
        .infor_txt
            p What do you need?
        .infor_data.infor_need
            input(type='button' value='Lose Weight')
            input(type='button' value='Gain Muscle')
            input(type='button' value='Get fitter')
        .col
        .infor_txt
            p Which parts should you focus on?
        .infor_data.infor_focus
            input(type='button' value='Full Body')
            input(type='button' value='Arms')
            input(type='button' value='Back')
            input(type='button' value='Shoulders')
            input(type='button' value='Chest')
            input(type='button' value='Thight')
            input(type='button' value='Abs')
            input(type='button' value='Butt')
        .infor_OK
            input(type="submit" value="send")
